<div id="sum" style="display:none">2</div>

 <script type="text/javascript">
     //document.body.onload=load;
     load();
     function load()
     { 
         var number = document.getElementById("number").getElementsByTagName("li");
         var count=number.length;
         number[0].className="numberOn";//默认选中第1个
         document.getElementById("tab").getElementsByTagName("div")[0].className="slide_show on";//默认显示第1张
         for(var j=0;j<count;j++)
         {
             number[j].onmousemove =function(){select(this)};//为每个li添加选择事件
         }
         setInterval(changePic,5*1000);//每5秒钟调用一次 
     }

     //图片选择菜单事件
     function select(object)
     {
         var start=object.innerHTML;
         display(start);
         document.getElementById("sum").innerHTML=object.innerHTML;
     }
      
     //切换图片
      function changePic(start)
      {        
        var object=document.getElementById("sum");
        start=object.innerHTML;
        display(start);
        object.innerHTML=parseInt(object.innerHTML)+1;//计数器,保存下一张的数值
        if(object.innerHTML>document.getElementById("tab").getElementsByTagName("div").length)
        {
        object.innerHTML="1";//计数器重置
        }
      }
      
      //隐藏切换
      function display(start)
      {
          var pic = document.getElementById("tab").getElementsByTagName("div");
          var number = document.getElementById("number").getElementsByTagName("li");
          var count=pic.length;
 
         for(var j=1;j<=count;j++)
         {
             if(j==start)
             {
                 pic[j-1].className="slide_show on";
                 number[j-1].className="numberOn";
             }
             else
             {
                 pic[j-1].className="slide_show";
                 number[j-1].className="";
             }
         }
      }
    </script>
